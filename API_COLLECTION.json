{
  "info": {
    "name": "Loan Management System API Collection",
    "description": "Complete API collection for the Loan Management System",
    "version": "1.0.0",
    "baseUrl": "http://localhost:8080"
  },
  "apis": [
    {
      "category": "Admin Authentication",
      "endpoints": [
        {
          "name": "Admin Register",
          "method": "POST",
          "url": "/api/admin/auth/register",
          "authentication": "None",
          "description": "Register a new admin user",
          "requestBody": {
            "username": "admin123",
            "email": "admin@example.com",
            "password": "admin123456"
          },
          "responseExample": {
            "adminId": 1,
            "username": "admin123",
            "email": "admin@example.com",
            "message": "Admin registered successfully"
          }
        },
        {
          "name": "Admin Login",
          "method": "POST",
          "url": "/api/admin/auth/login",
          "authentication": "None",
          "description": "Login as admin and get JWT token",
          "requestBody": {
            "usernameOrEmail": "admin123",
            "password": "admin123456"
          },
          "responseExample": {
            "adminId": 1,
            "username": "admin123",
            "email": "admin@example.com",
            "accessToken": "eyJhbGciOiJIUzI1NiJ9...",
            "tokenType": "Bearer"
          }
        }
      ]
    },
    {
      "category": "Applicant Authentication",
      "endpoints": [
        {
          "name": "Applicant Register",
          "method": "POST",
          "url": "/api/applicant/auth/register",
          "authentication": "None",
          "description": "Register a new applicant and receive OTP via email",
          "requestBody": {
            "firstName": "John",
            "lastName": "Doe",
            "dob": "1990-05-15",
            "gender": "Male",
            "email": "john.doe@example.com",
            "phone": "9876543210",
            "password": "password123",
            "address": "123 Main Street, Apt 4B",
            "city": "Mumbai",
            "state": "Maharashtra",
            "country": "India"
          },
          "responseExample": {
            "message": "Registration initiated. Please verify your email with the OTP sent to john.doe@example.com"
          }
        },
        {
          "name": "Verify OTP",
          "method": "POST",
          "url": "/api/applicant/auth/verify-otp",
          "authentication": "None",
          "description": "Verify email with OTP code",
          "requestBody": {
            "email": "john.doe@example.com",
            "otp": "123456"
          },
          "responseExample": {
            "message": "Email verified successfully. Your registration is pending admin approval."
          }
        },
        {
          "name": "Resend OTP",
          "method": "POST",
          "url": "/api/applicant/auth/resend-otp",
          "authentication": "None",
          "description": "Resend OTP to email",
          "queryParams": {
            "email": "john.doe@example.com"
          },
          "responseExample": {
            "message": "OTP resent successfully to john.doe@example.com"
          }
        },
        {
          "name": "Applicant Login",
          "method": "POST",
          "url": "/api/applicant/auth/login",
          "authentication": "None",
          "description": "Login as applicant (requires email verification and admin approval)",
          "requestBody": {
            "email": "john.doe@example.com",
            "password": "password123"
          },
          "responseExample": {
            "applicantId": 1,
            "firstName": "John",
            "lastName": "Doe",
            "email": "john.doe@example.com",
            "accessToken": "eyJhbGciOiJIUzI1NiJ9...",
            "tokenType": "Bearer",
            "isApproved": true,
            "approvalStatus": "APPROVED",
            "message": "Login successful"
          }
        }
      ]
    },
